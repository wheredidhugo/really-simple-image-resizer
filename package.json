{
  "name": "really-simple-image-resizer",
  "version": "1.0.0",
  "description": "Just a simple image resizer.",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . really-simple-image-resizer --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Really Simple Image Resizer\"",
    "package-linux": "electron-packager . really-simple-image-resizer --overwrite --asar=true --platform=linux --arch=x64 --icon=src/icon.png --prune=true --out=release-builds"
  },
  "author": "bgpvp",
  "license": "ISC",
  "dependencies": {
    "electron": "^16.0.4",
    "electron-packager": "^15.4.0",
    "sharp": "^0.29.3"
  }
}
