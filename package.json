{
  "name": "really-simple-image-resizer",
  "version": "1.1.0",
  "description": "Just a simple image resizer.",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/icon.icns --prune=true --out=release-builds",
    "package-win": "npx electron-packager . --overwrite --icon=src/icon.ico",
    "package-linux": "electron-packager . really-simple-image-resizer --overwrite --platform=linux --arch=x64 --icon=src/icon.png --prune=true --out=release-builds"
  },
  "license": "ISC",
  "dependencies": {
    "electron": "^16.0.6",
    "electron-packager": "^15.4.0",
    "sharp": "^0.29.3"
  }
}
